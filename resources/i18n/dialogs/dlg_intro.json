                "offset": [
                    119.336342,
                    -71.512917
                ],
                "slide_camera": true,
                "speaker_type": 0,
                "text": {
                    "ENG": "...who are you?",
                    "SPA": "...¿quién eres?"
                },
                "title": "Show Message"
            },
            {
                "character": [
                    "Player",
                    0
                ],
                "expand": "[GraphNode:3864]",
                "expand_size": [
                    0,
